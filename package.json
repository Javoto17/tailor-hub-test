{
  "name": "myapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "e2e:build": "detox build",
    "e2e:test": "detox test",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:build:ios:release": "detox build --configuration ios.sim.release",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test:ios:release": "detox test --configuration ios.sim.release",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:build:android:release": "detox build --configuration android.emu.release",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:test:android:release": "detox test --configuration android.emu.release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.1",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-vector-icons/common": "^11.0.0",
    "@react-native-vector-icons/ionicons": "^7.4.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@react-navigation/stack": "^7.1.1",
    "@tanstack/react-query": "^5.66.8",
    "just-debounce-it": "^3.2.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-native": "0.77.0",
    "react-native-gesture-handler": "~2.23.1",
    "react-native-image-picker": "^8.1.0",
    "react-native-maps": "^1.20.1",
    "react-native-reanimated": "~3.16.7",
    "react-native-safe-area-context": "^5.2.0",
    "react-native-screens": "4.6.0",
    "react-native-svg": "^15.11.1",
    "tailwind-variants": "^0.3.1",
    "tailwindcss": "^3.4.17",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.77.0",
    "@react-native/eslint-config": "0.77.0",
    "@react-native/metro-config": "0.77.0",
    "@react-native/typescript-config": "0.77.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "20.33.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "prettier": "3.5.0",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.2.5",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
